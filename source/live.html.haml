%section.live-embed#js-live-embed
:javascript
  var section = document.getElementById('js-live-embed');
  var iframe = document.createElement('iframe');
  iframe.setAttribute('allowfullscreen', 'true');
  var search = window.location.search || '';
  var mqWidth = {
    xs: 0,
    sm: 576,
    md: 768,
    lg: 992,
    xl: 1200,
    '2xl': 1300,
    '3xl': 1500,
  };

  function setHeight () {
    var height = window.innerHeight;
    var width = window.innerWidth;
    if (width < mqWidth.xl) {
      // this is the width when the chat is below the player
      // we want a very tall height because all the chat is underneath the player
      iframe.setAttribute('height', '2000');
    } else {
      // subtract a bit for the top nav and bottom footer
      height = height - 200;
      if (height < 900) {
        height = '900';
      }
      iframe.setAttribute('height', height);
    }
  }
  iframe.setAttribute('src', 'https://live-2020-demuxed-com.vercel.app/' + search);
  section.appendChild(iframe);

  setHeight();

  window.onresize = setHeight;
